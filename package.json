{
  "name": "ts-express-decorators",
  "version": "3.0.0",
  "description": "A TypeScript Framework on top of Express",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf 'test/{units,integration,helper}/**/*.{js,js.map}' 'src/**/*.{js,js.map}'",
    "tslint": "tslint 'src/**/*.ts?(x)'",
    "test": "npm run clean && npm run tsc && npm run tslint && NODE_ENV=test nyc --reporter=html --reporter=text _mocha --recursive",
    "coveralls:travis": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:compile": "rimraf ./lib && tsc --project tsconfig.compile.json",
    "doc:serve": "concurrently \"tsc --project tsconfig.compile.json -w\" \"docsify serve ./docs\" \"cd docs/_build && nodemon --delay 2.5 -e ts --watch ../../lib build-api.js\"",
    "doc:build": "cd docs/_build && node build-api.js && cd ../..",
    "semantic-release-locale": "semantic-release pre --debug=true",
    "prepublish": "npm run tsc:compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Romakita/ts-express-decorators.git"
  },
  "keywords": [
    "Express",
    "TypeScript",
    "typescript",
    "Decorator",
    "decorators",
    "decorator",
    "express",
    "Controller",
    "Inject",
    "ioc",
    "di",
    "mvc",
    "swagger",
    "swagger ui", "ES2015", "ES6", "server",
    "rest",
    "api",
    "validation"
  ],
  "author": "Romain Lenzotti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Romakita/ts-express-decorators/issues"
  },
  "homepage": "https://romakita.github.io/ts-express-decorators/#/",
  "dependencies": {
    "@types/json-schema": "^6.0.0",
    "@types/swagger-schema-official": "^2.0.7",
    "glob": "^7.1.2",
    "json-schema": "^0.2.3",
    "reflect-metadata": "^0.1.10",
    "source-map-support": "^0.5.0",
    "ts-httpexceptions": "^3.0.0",
    "ts-log-debug": "^4.0.1",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/chai-as-promised": "7.1.0",
    "@types/cookie-parser": "^1.3.29",
    "@types/express": "^4.0.32",
    "@types/mocha": "^2.2.31",
    "@types/multer": "^1.3.6",
    "@types/node": "^8.0.32",
    "@types/proxyquire": "^1.3.27",
    "@types/sinon": "^2.2.1",
    "@types/sinon-chai": "^2.7.27",
    "@types/socket.io": "^1.4.31",
    "@types/superagent": "^3.5.3",
    "@types/supertest": "^2.0.0",
    "ajv": "^5.2.4",
    "body-parser": "^1.15.1",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "compression": "^1.6.1",
    "concurrently": "^3.5.0",
    "cookie-parser": "^1.4.1",
    "coveralls": "^3.0.0",
    "docsify-cli": "^4.1.9",
    "docsify-server-renderer": "^4.2.6",
    "ejs": "^2.5.5",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "fs-extra": "^4.0.0",
    "html-minifier": "^3.5.2",
    "method-override": "^2.3.10",
    "mocha": "^4.0.1",
    "multer": "^1.3.0",
    "nyc": "^11.4.0",
    "proxyquire": "^1.8.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^7.0.2",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "socket.io": "^2.0.4",
    "supertest": "^3.0.0",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "swagger-ui-express": "^2.0.11",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.5.0"
  },
  "directories": {
    "lib": "lib",
    "src": "src",
    "test": "test"
  },
  "release": {
    "debug": false,
    "branch": "production"
  },
  "gflow": {
    "master": "master",
    "production": "production"
  }
}
